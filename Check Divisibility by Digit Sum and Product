class Solution {
    public boolean checkDivisibility(int n) {
        int sum = 0, mul = 1, temp = n;
        while (temp != 0) {
            int dig = temp % 10;
            sum += dig;
            mul *= dig;
            temp /= 10;
        }
        return (sum + mul != 0 && n % (sum + mul) == 0);
    }
}
