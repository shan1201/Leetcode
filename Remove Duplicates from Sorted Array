class Solution {
    public int removeDuplicates(int[] nums) {
        int k=0;
        int i=1;
        int len=nums.length;
            for(int j=1;j<len;j++){
                if(nums[j]!=nums[i-1]){
                    nums[i]=nums[j];
                    i++;
                }
            }
        return i;
    }
}
