class Solution {
    public double findMaxAverage(int[] nums, int k) {
        double avg=0;
        double sum=Double.MIN_VALUE;
        for(int i=0;i<k;i++){
            sum+=nums[i];
        }
        avg=sum/k;
        for(int i=k;i<nums.length;i++){
            double temp=avg;
            sum+=nums[i]-nums[i-k];
            temp=sum/k;
            avg=Math.max(temp,avg);
        }
        return avg;
    }
}
